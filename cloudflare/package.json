{
  "name": "leetcode-study-api",
  "version": "1.0.0",
  "description": "Cloudflare Workers API for LeetCode Study Guide with D1 database",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "db:create": "wrangler d1 create leetcode-study-db",
    "db:migrate": "wrangler d1 migrations apply leetcode-study-db",
    "db:migrate:dev": "wrangler d1 migrations apply leetcode-study-db --local",
    "db:shell": "wrangler d1 execute leetcode-study-db --command"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "itty-router": "^4.0.0"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}
